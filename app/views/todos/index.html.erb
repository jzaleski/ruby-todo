<h1>Todos</h1>

<table class="list_view" cellpadding="0" cellspacing="0">
<% @todos.each do |todo| %>
	<%= form_for todo, :format => :json, :remote => true do |f| %>
	<tr>
		<td><%= check_box 'todo', 'completed' %></td>
		<td></td>
		<td><%= f.text_field :summary, :autocomplete => 'off' %></td>
	</tr>
	<% end %>
<% end %>
</table>
